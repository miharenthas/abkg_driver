#COMPILE=f77 -DCERNLIB_TYPE -g2 -c -O0 -check bounds -extend_source -I/us1/cern/98/include/geant321 -I/us1/cern/98/include
COMPILE=f77 -DCERNLIB_TYPE -g0 -c -O2 -ffixed-line-length-none
LINK=f77 -g0 -O2

all: abkg 

abkg: abkg.o
	$(LINK) -o abkg abkg.o -L/cern/2005-x86-64/lib -lpacklib

%.o : %.F
	$(COMPILE) $<

clean:
	rm abkg.o abkg


